class POLYNOMIAL{
    private integer b;
    public func evaluate(integer x) : integer;
};

func POLYNOMIAL::evaluate(integer x) : integer
{
  var
  {
    integer bb;
  }
  bb = 1;
  return(bb+x);
}


main
{
  var
  {
    POLYNOMIAL polynomial1;
    integer a;
  }
